#include <iostream>
using namespace std;
int main() { 
int a , b;
cout<<"Enter two numbers : "<<endl;
cin>>a>>b;

char op;
cout<<"Enter the operation"<<endl;
cin>>op;


switch (op) {
case '+' : cout<<a+b<<endl;
     break;
case '-' : cout<<a-b<<endl;
     break;
case '*' : cout<<a*b<<endl;
     break;
case '/' : cout<<a/b<<endl;
     break;
case '%' : cout<<a%b<<endl;
     break;

default :
cout<<"Plz enter valid operation"<<endl;
}



return 0;
}
