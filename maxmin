#include <iostream>
using namespace std;
int main() { 

int getMax (int[], int n) {
    int maxi = INT_MIN;
    maxi = max(maxi, num[i] )
    return mini;
}

int getMin (int[], int n) {
    int mini = INT_MAX;
    mini = max(mini, num[i] )
    return maxi;
}

int size;
cin>>size;

int num[100];
for (int i =0;i<size;i++) {
    cin>>num[i]; 
}

